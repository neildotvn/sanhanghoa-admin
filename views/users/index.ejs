<%- include ('../partials/header') %>
<table>
    <tr>
        <td>STT</td>    
        <td>Tên</td>    
        <td>SĐT</td>    
        <td>Địa chỉ</td>    
        <td>Email</td>    
        <td>Giới tính</td>    
    </tr>
    <% users.forEach((user, index) => { %>
        <tr>
            <td><%= index + 1 %></td>    
            <td><%= user.full_name %></td>    
            <td><%= user.phone %></td>    
            <td><%= user.address %></td>    
            <td><%= user.email %></td>    
            <td><%= user.gender %></td>    
        </tr>
    <% }) %>
</table>
<%- include ('../partials/footer') %>
